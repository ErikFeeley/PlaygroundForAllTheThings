@model IEnumerable<ToDoThing>

<div class="jumbotron text-center">
    <h1>ToDos!</h1>
    <div>
        <a asp-controller="ToDo" asp-action="Add" class="btn btn-lg btn-primary">Add a ToDo!</a>
    </div>
</div>

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        <div>
            <h4 class="text-center">Where the todos go yo.</h4>
        </div>
    </div>
</div>

<hr />

<div class="row">
    <div class="col-md-6 col-md-offset-3">
        
        @if (@Model != null)
        {
            foreach (var toDoThing in Model)
            {
                <h1>@toDoThing.Title</h1>
            }
        }
    </div>
</div>